keep_idx <- complete.cases(st_drop_geometry(flowlines_matched))
cat("Rows with any remaining NA (non-geometry):", sum(!keep_idx), "\n")
flowlines_matched <- flowlines_matched[keep_idx, , drop = FALSE]
